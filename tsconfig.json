{
  "compilerOptions": {
    "target": "es2021",
    "lib": ["dom", "dom.iterable", "esnext", "es2021"],
    "types": ["node", "jest", "cypress"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "tsBuildInfoFile": "tsconfig.tsbuildinfo"
  },
  "include": ["next-env.d.ts"],
  "exclude": [
    ".build",
    "node_modules",
    "components",
    "functions",
    "pages",
    "tests",
    "types",
    "utils",
    "index.ts",
    "binaryMimeTypes.ts",
    "server.ts"
  ]
}
